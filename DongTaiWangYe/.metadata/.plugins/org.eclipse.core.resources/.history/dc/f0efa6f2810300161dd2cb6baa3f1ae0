package random;

import java.util.Random;

public class RandomClass {
	
public static void main(String[] args) {
	Random rand =  new Random(10);
	for(int i=0; i<5; i++)
		System.out.println(rand.nextInt());
	System.out.println("\n");
	Random rand1 = new Random(10);
	for(int i=0; i<5; i++)
		System.out.println(rand.nextInt());
}
}
