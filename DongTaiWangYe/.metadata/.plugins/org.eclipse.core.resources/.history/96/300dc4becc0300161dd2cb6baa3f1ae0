package InitClass;

public class Person {
	
public static class Home{
	Home(){}
	void A()
	{
		System.out.println("这是静态内部类的实例方法");
	}
	
	static void B()
	{
		System.out.println("这是静态内部类的类方法");
	}
}
}

class Output{
	public static void main(String[] args) {
		Person p = new Person();
		
		Home h = new Home();
		
		Person.Home.B();
	}
}