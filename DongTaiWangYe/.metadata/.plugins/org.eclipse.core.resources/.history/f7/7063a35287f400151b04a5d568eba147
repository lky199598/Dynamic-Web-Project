package InitClass;
	
class Mug{
	Mug(int marker)
	{
		System.out.println("Mug("+marker+")");
	}

}


public class Mugs
{
	Mug mug1;
	Mug mug2;
	{
		mug1 = new Mug(1);
		mug2 = new Mug(2);
	}
	
	Mugs(){
		System.out.println("Mugs()");
	}
	
	Mugs(int n)
	{
		System.out.println("Mugs("+n+")");
	}
	
	
	public static void main(String[] args) {
		
	}
}

