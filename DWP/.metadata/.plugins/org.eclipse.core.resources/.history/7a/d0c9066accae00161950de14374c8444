public class SpringDemo1 {
    
	//测试业务层的类
	//@Resource:使用字段名字匹配bean,查找到bean则注入,如果类型不匹配则此时有异常,注入失败。
	@Resource(name="accountService")
	private AccountService accountService;
	
	@Test
    public void demo1()
    {
    	
    }
}
